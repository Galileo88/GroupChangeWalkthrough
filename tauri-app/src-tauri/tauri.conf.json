{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Provider Enrollment Walkthrough",
  "version": "1.0.1",
  "identifier": "com.providerenrollment.walkthrough",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "../src"
  },
  "app": {
    "windows": [
      {
        "title": "Provider Enrollment Walkthrough",
        "width": 900,
        "height": 900,
        "minWidth": 900,
        "minHeight": 900,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "resources/*.pdf"
    ],
    "windows": {
      "nsis": {
        "perMachine": false,
        "allowDowngrade": true,
        "displayLanguageSelector": false
      }
    }
  },
  "plugins": {
    "updater": {
