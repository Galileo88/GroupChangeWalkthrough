{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Provider Enrollment Walkthrough",
  "version": "1.0.0",
  "identifier": "com.providerenrollment.walkthrough",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "../src"
  },
  "app": {
    "windows": [
      {
        "title": "Provider Enrollment Walkthrough",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "../resources/2025LegalQuickGuide.pdf": "resources/",
      "../resources/ProviderTypesSpecialties.pdf": "resources/",
      "../resources/FLOW_MAPPING.pdf": "resources/"
    }
  }
}
